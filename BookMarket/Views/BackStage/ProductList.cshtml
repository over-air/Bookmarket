@model IList<ViewModels.BackStore.ProductStore>
@{
    ViewBag.Title = "ProductList";
    Layout = "~/Views/Shared/_LayoutBack.cshtml";
}

<div class="row">
    <div class="col-md-12">
        <table class="table">
            <tr>
                <td>商品名称</td>
                <td>操作</td>
            </tr>
            @foreach (var product in Model)
            {
                <tr>
                    <td>@product.Product.Name</td>
                    <td><a>编辑</a></td>
                </tr>
                
            }
        </table>
    </div>
</div>

